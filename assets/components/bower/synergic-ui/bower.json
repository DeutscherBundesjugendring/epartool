    "/src/js/tests",
    "/src/styleguide",
    "/src/less/styleguide.md"
  ],
  "dependencies": {
    "bootstrap": "~3.3.0",
    "less-modules": "~0.3.0",
    "eonasdan-bootstrap-datetimepicker": "~3.1.3",
    "select2": "~3.5.2",
    "select2-bootstrap-css": "~1.4.1"
  },
  "devDependencies": {
    "qunit": "~1.15.0"
  }
